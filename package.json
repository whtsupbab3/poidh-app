{
  "name": "ts-nextjs-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:strict": "eslint --max-warnings=98 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@farcaster/frame-sdk": "^0.0.16",
    "@mui/material": "^5.15.18",
    "@mui/x-charts": "^7.5.1",
    "@prisma/client": "^5.21.1",
    "@rainbow-me/rainbowkit": "^2.2.0",
    "@tanstack/react-query": "^5.59.15",
    "@trpc/client": "11.0.0-rc.592",
    "@trpc/next": "11.0.0-rc.592",
    "@trpc/react-query": "11.0.0-rc.592",
    "@trpc/server": "11.0.0-rc.592",
    "browser-image-compression": "^2.0.2",
    "frames.js": "^0.20.0",
    "@types/react-infinite-scroller": "^1.2.5",
    "axios": "^1.6.8",
    "clsx": "^2.0.0",
    "framer-motion": "^11.0.12",
    "mobx": "^6.12.3",
    "next": "13.5.3",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-infinite-scroller": "^1.2.5",
    "react-minimal-pie-chart": "^9.1.0",
    "react-toastify": "^10.0.5",
    "superjson": "^2.2.1",
    "supports-color": "8.1.1",
    "tailwind-merge": "^2.3.0",
    "viem": "^2.12.1",
    "wagmi": "^2.12.29",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.3.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.19",
    "encoding": "^0.1.13",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "next-router-mock": "^0.9.13",
    "next-sitemap": "^2.5.28",
    "postcss": "^8.4.38",
    "prettier": "^2.8.8",
    "prisma": "^5.21.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=98",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ]
  }
}
